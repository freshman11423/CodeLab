{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": ".",
    "module": "esnext",
    "moduleResolution": "bundler",
    "jsx": "react-native",
    "paths": {
      "@/*": ["./*"],
      "app/*": ["./app/*"]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx"
  ]
}
